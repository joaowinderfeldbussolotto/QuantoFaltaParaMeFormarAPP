<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.dev.joaobussolotto.mobile.quantofaltaprameformar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuantoFaltaPraMeFormar">
        <activity
            android:name=".ui.activity.AddComplementarActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.AddUniversityActivity"
            android:exported="false"
            android:label="@string/title_activity_add_new_subject"
            android:theme="@style/Theme.QuantoFaltaPraMeFormar.NoActionBar" />
        <activity
            android:name=".ui.activity.AddSubjectActivity"
            android:exported="false"
            android:label="@string/title_activity_add_new_subject"
            android:theme="@style/Theme.QuantoFaltaPraMeFormar.NoActionBar" />
        <activity
            android:name=".ui.activity.MainActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>